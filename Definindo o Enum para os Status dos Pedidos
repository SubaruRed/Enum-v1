public enum StatusPedido {
    PENDENTE("Pedido pendente"),
    EM_TRANSPORTE("Em transporte"),
    ENTREGUE("Entregue"),
    CANCELADO("Cancelado");

    private final String descricao;

    StatusPedido(String descricao) {
        this.descricao = descricao;
    }

    public String getDescricao() {
        return descricao;
    }
}
